  <script>
    document.addEventListener("DOMContentLoaded", function(){
      setTimeout(() => {
        

        $('.jdgm-carousel-title').replaceWith(function() {
          return $("<span>", {
            class: this.className,
            html: $(this).html()
          });
        });

        
        $('.jdgm-rev-widg__title').replaceWith(function() {
          let e = $("<div>", {
            class: this.className,
            html: $(this).html()
          });
          e.addClass("h2");
          return e;
        });
      }, "1000")
    });
    </script>  
